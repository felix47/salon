					<option value="solo">Vertical Centered</option>
					<!--option value="none">None</option-->
				</field>
				<field name="button_style" type="list" default="round" label="Navigation Style" description="Navigation Style">
					<option value="round">Round</option>
					<option value="square">Square</option>
					<option value="navbar">Navbar</option>
				</field>
				<field name="navigationhalign" type="list" default="center" label="Navigation Vertical Align" description="Navigation Vertical Align">
					<option value="left">Left</option>
					<option value="center">Center</option>
					<option value="right">Right</option>
				</field>
				<field name="navigationvalign" type="list" default="bottom" label="Navigation Horizontal Align" description="Navigation Horizontal Align">
					<option value="top">Top</option>
					<option value="center">Center</option>
					<option value="bottom">Bottom</option>
				</field>
				<field name="shownavigation" class="btn-group" type="radio" default="bullet" label="Show Navigation" description="Show Navigation">
					<option value="0">No</option>
					<option value="bullet">Yes</option>
				</field>
				<field name="pause" class="btn-group" type="radio" default="1" label="Stop When Hover" description="Stop when hover">
					<option value="0">No</option>
					<option value="1">Yes</option>
				</field>
				<field name="interval" type="text" label="Interval" default="4000" description="Speed of slideshow. Larger = Slower" />
				<field name="timer_position" class="btn-group" type="radio" default="top" label="Timer Position" description="Timer Position">
					<option value="top">Top</option>
					<option value="under">Under</option>
				</field>
				
